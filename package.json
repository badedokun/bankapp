{
  "name": "OrokiiPayApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:frontend": "jest --selectProjects=\"React Native\"",
    "test:backend": "jest --selectProjects=\"Backend\"",
    "test:integration": "jest --selectProjects=\"Integration\"",
    "test:ux": "jest --testPathPattern=tests/ux",
    "test:e2e": "playwright test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:all": "npm run test:frontend && npm run test:backend && npm run test:integration && npm run test:ux && npm run test:e2e",
    "test:feature": "npm run test:frontend -- --findRelatedTests && npm run test:integration -- --findRelatedTests && npm run test:ux -- --findRelatedTests",
    "test:pre-commit": "npm run test:frontend && npm run test:integration",
    "test:pre-push": "npm run test:all",
    "web": "webpack serve --mode development",
    "web:build": "webpack --mode production",
    "web:dev": "webpack serve --mode development --open",
    "dev": "concurrently \"npm run start\" \"npm run web:dev\"",
    "build": "webpack --mode production && react-native bundle --platform ios --entry-file index.js --bundle-output ios/main.jsbundle",
    "server": "ts-node server/index.ts",
    "server:dev": "nodemon --exec ts-node server/index.ts",
    "server:build": "tsc -p server/tsconfig.json",
    "server:prod": "npm run server:build && node dist/server/index.js",
    "start:prod": "node dist/server/index.js",
    "db:setup": "ts-node scripts/setup-database.ts",
    "db:seed": "ts-node scripts/seed-data.ts",
    "db:reset": "ts-node scripts/setup-database.ts --reset",
    "provision-tenant": "ts-node scripts/provision-tenant-database.ts"
  },
  "dependencies": {
    "@babel/preset-react": "^7.27.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/new-app-screen": "0.81.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@react-navigation/stack": "^7.4.8",
    "@reduxjs/toolkit": "^2.9.0",
    "@tanstack/react-query": "^5.86.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.11.0",
    "babel-loader": "^10.0.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "html-webpack-plugin": "^5.6.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "natural": "^8.1.0",
    "openai": "^5.19.1",
    "path-browserify": "^1.0.1",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.81.1",
    "react-native-config": "^1.5.9",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-keychain": "^10.0.0",
    "react-native-paper": "^5.14.5",
    "react-native-paper-alerts": "^0.1.3",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-web": "^0.21.1",
    "react-redux": "^9.2.0",
    "sqlite3": "^5.1.7",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
